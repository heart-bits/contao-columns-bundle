            <trans-unit id="tl_form_field.fields.column-push-pull.0">
                <source>Shift column</source>
                <target>Spalte verschieben</target>
            </trans-unit>
            <trans-unit id="tl_form_field.fields.column-push-pull.1">
                <source>Choose if the column should be pulled to the left or pushed to the right.</source>
                <target>Wählen Sie, ob die Spalte nach link oder rechts verschoben werden soll.</target>
            </trans-unit>
            <trans-unit id="tl_form_field.fields.pull-push-breakpoint.0">
                <source>Breakpoint</source>
                <target>Breakpoint</target>
            </trans-unit>
            <trans-unit id="tl_form_field.fields.pull-push-breakpoint.1">
                <source>Choose the breakpoint, at which the column should be shifted.</source>
                <target>Wählen Sie den breakpoint, ab dem die Spalte verschoben werden soll.</target>
            </trans-unit>
            <trans-unit id="tl_form_field.fields.pull-push-size.0">
                <source>Size of shift</source>
                <target>Verschiebungs-Weite</target>
            </trans-unit>
            <trans-unit id="tl_form_field.fields.pull-push-size.1">
                <source>Choose the size, the column should be shifted.</source>
                <target>Wählen Sie die Weite, um die die Spalte verschoben werden soll.</target>
            </trans-unit>
            <trans-unit id="tl_form_field.push-pull.pull">
                <source>Pull Column to the left.</source>
                <target>Die Spalte nach links ziehen</target>
            </trans-unit>
            <trans-unit id="tl_form_field.push-pull.push">
                <source>Push column to the right.</source>
                <target>Die Spalte nach rechts schieben</target>
            </trans-unit>
            <trans-unit id="tl_form_field.breakpoint.small">
                <source>Mobile</source>
                <target>Mobile</target>
            </trans-unit>
            <trans-unit id="tl_form_field.breakpoint.medium">
                <source>Tablet</source>
                <target>Tablet</target>
            </trans-unit>
            <trans-unit id="tl_form_field.breakpoint.large">
                <source>Desktop</source>
                <target>Desktop</target>
            </trans-unit>
